#include "../common/STD_PKT_COMMON.def"

//
// SCTP CHUNK_INIT
//
PKG_SCTP_EXTH_SEND(
	sctp_chunk_init_bad_tag,
	{
		SourcePort = SCTP_TN0_PORT;
		DestinationPort = SCTP_NUT0_PORT;
		VerificationTag = 100;
	},
	{
		chunk = chunk_init_snd;
	}
)

//
// SCTP CHUNK_ABORT
//
PKG_SCTP_SHORT_RECV(
	sctp_chunk_abort_bad_tag,
	{
		chunk = chunk_abort_bad_tag;
	}
)

CHUNK_ABORT chunk_abort_bad_tag {
	TFlag = 0;
	Length = stop;
}
